<% description = "Aranet es una empresa de servicios de limpieza y mantenimientos en Badalona que ofrece limpieza profesional para oficinas, hogares, comunidades, parking y garajes. Servicios de limpieza en Badalona, Santa Coloma de Gramanet, Sant Adrià de Besòs y provincia de Barcelona." %>
<% title = "Aranet servicios de limpieza y mantenimientos en Badalona" %>

<%= tag "meta", "http-equiv" => "Content-Type", content: "text/html" %>
<%= tag "meta", name: "resource-type", content: "document" %>
<%= tag "meta", name: "lang", content: I18n.locale %>
<%= tag "meta", name: "DC.language", scheme: "RFC1766", content: I18n.locale %>
<%= tag "meta", name: "robots", content: "all" %>
<%= tag "meta", name: "charset", content: "utf-8" %>
<%= tag "meta", name: "viewport", content: "width=device-width, initial-scale=1.0" %>
<%= tag "meta", name: "description", content: description %>
<%= tag "meta", name: "DC.Description", content: description %>
<%= tag "meta", name: "author", content: "Aranet" %>
<%= tag "meta", property: "og:locale", content: "es_ES" %>
<%= tag "meta", property: "og:locale:alternate", content: "ca_ES" %>
<%= tag "meta", property: "og:type", content: "business.business" %>
<%= tag "meta", property: "og:title", content: title %>
<%= tag "meta", property: "og:description", content: description %>
<%= tag "meta", property: "og:url", content: "http://aranetbadalona.es/" %>
<%= tag "meta", property: "og:site_name", content: "Aranet" %>
<%= tag "meta", property: "og:image", content: asset_url("blue_logo.png") %>
<%= tag "meta", name: "twitter:card", content: "summary" %>
<%= tag "meta", name: "twitter:description", content: description %>
<%= tag "meta", name: "twitter:title", content: title %>
